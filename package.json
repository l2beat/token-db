{
  "name": "@l2beat/token-db",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "run-s build:*",
    "build:1-generate": "prisma generate",
    "build:2-compile": "tsc",
    "clean": "rm -rf build",
    "dev": "prisma generate && tsx --no-warnings=ExperimentalWarning --watch src/main.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx src/db/seed.ts",
    "format": "biome check .",
    "format:fix": "biome format --write .",
    "lint": "eslint --ext .ts --max-warnings 0 src",
    "lint:fix": "eslint --ext .ts --max-warnings 0 src --fix",
    "nanoid": "nanoid",
    "start": "tsx src/main.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@l2beat/backend-tools": "^0.5.2",
    "@prisma/client": "5.13.0",
    "dotenv": "^16.4.5",
    "nanoid": "^5.0.7",
    "postgres": "^3.4.4",
    "viem": "^2.10.1",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@types/node": "^20.12.10",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.13.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.9.3",
    "type-fest": "^4.18.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
